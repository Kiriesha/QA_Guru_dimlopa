				loadCss: function(url) {
					if (document.querySelector('link[href="' + url + '"]')) {
						return;
					}
					var link = document.createElement("link");
					link.type = "text/css";
					link.rel = "stylesheet";
					link.href = url;
					document.getElementsByTagName("head")[0].appendChild(link);
				},
				loadJs: function (url, callback) {
					if (document.querySelector('script[src="' + url + '"]')) {
						if (callback) {
							callback();
						}
						return;
					}
					var script = document.createElement('script');
					script.type = 'text/javascript';
					script.src = url;
					if (callback) {
						script.onload = function () {
							callback();
						};
					}
					document.body.appendChild(script);
				},
				changeSearchPlaceholder: function (id) {
					// [164717] [WWW, PDA] Верстка вариативности текста в поисковом инпуте
					var searchInput = document.getElementById(id);
					if (searchInput) {
						var inputWidth = searchInput.offsetWidth;
						var placeholder = 'Книга, серия, автор, жанр, тег, издательство';
						if (inputWidth <= 250) {
							placeholder = 'Книга, серия, автор';
						}
						if (inputWidth > 250 && inputWidth <= 400) {
							placeholder = 'Книга, серия, автор, жанр, тег';
						}
						searchInput.setAttribute("placeholder", placeholder);
					}
				},
				requireFilter: function(url) {
					if (!/\?/.test(url) && /\.js$/.test(url)) {
						url = url.slice(0, -3);
					}
					return url;
				},
