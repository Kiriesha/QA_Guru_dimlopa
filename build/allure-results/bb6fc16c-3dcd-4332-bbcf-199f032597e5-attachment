          // Google Analitycs
          window.dataLayer = window.dataLayer || [];
          function gtag() {
            window.dataLayer.push(arguments);
          }
          gtag('js', new Date());

          // –ú–µ–π–ª.—Ä—É —Å—á–µ—Ç—á–∏–∫ - –ø–µ—Ä–µ–º–µ–Ω–Ω–∞—è —Å–±–æ—Ä–∞ –¥–∞–Ω–Ω—ã—Ö
          window._tmr = window._tmr || [];

