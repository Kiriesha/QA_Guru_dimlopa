				loadJs: function (url, callback) {
					if (document.querySelector('script[src="' + url + '"]')) {
						if (callback) {
							callback();
						}
						return;
					}
					var script = document.createElement('script');
					script.type = 'text/javascript';
					script.src = url;
					if (callback) {
						script.onload = function () {
							callback();
						};
					}
