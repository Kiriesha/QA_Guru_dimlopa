					var ymSended = false;
					function onCoverError(coverEl) {
						coverEl.dataset.coverError = 1;

						if (!ymSended) {
							ymReachGoal("webp_convert_error", {
								artId: litres.page.biblio_book.art.id
							});

							ymSended = true;
						}
					}

					(function () {
						function drawCover() {
							var coverWrap = document.createElement('picture');
							var cover = document.createElement('img');
							var windowWidth = document.body.offsetWidth;
							var width = 0;
							var height = 0;
							var img_path = 'https://cv9.litres.ru/pub/c/elektronnaya-kniga/cover/63113692-antonina-kreyn-sholoh-tenevye-bliki-63113692.jpg';
							if (windowWidth <= 1279 || false || false) {
								width = 330;
								height = 519;
							} else {
								
								width = 415;
								height = 653;
							}
							
							cover.setAttribute('id', 'biblio_book_cover_image');
							cover.setAttribute('class', '');
							cover.setAttribute('itemprop', 'image');
							cover.setAttribute('width', width);
							cover.setAttribute('height', height);
							cover.setAttribute('src', img_path.replace('/cover/','/cover_' + width + '/'));
							cover.setAttribute('onerror', 'onCoverError(this);');
							cover.dataset.src = 'https://cv9.litres.ru/pub/c/elektronnaya-kniga/cover/63113692-antonina-kreyn-sholoh-tenevye-bliki-63113692.jpg';
							cover.setAttribute('alt', '–®–æ–ª–æ—Ö. –¢–µ–Ω–µ–≤—ã–µ –±–ª–∏–∫–∏');

							var webpSource = document.createElement('source');
							webpSource.setAttribute('srcset', img_path.replace('/cover/','/cover_' + width + '/').replace('.jpg', '.webp'));
							webpSource.setAttribute('type', 'image/webp');
							coverWrap.appendChild(webpSource);

							var originalSource = document.createElement('source');
							originalSource.setAttribute('srcset', img_path.replace('/cover/','/cover_' + width + '/'));
							originalSource.setAttribute('type', 'image/jpg');
							coverWrap.appendChild(originalSource);

							coverWrap.appendChild(cover);
							document.getElementById('biblio-book-cover-wrapper').appendChild(coverWrap);
						}

						drawCover();
					})();
